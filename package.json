{
  "name": "swarm-wasp-starter",
  "type": "module",
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@ingenyus/swarm": "^0.4.0",
    "@ingenyus/swarm-wasp": "^0.3.0",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^4.1.14",
    "wasp": "file:.wasp/out/sdk/wasp"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/react": "^18.0.37",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.4.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "5.19.1",
    "rimraf": "^6.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.8.2",
    "vite": "^7.0.6",
    "wasp-config": "file:../../.local/share/wasp-lang/0.18.1/data/packages/wasp-config"
  },
  "scripts": {
    "start:wasp": "wasp ts-setup && wasp start",
    "start:db": "wasp db start",
    "reset:wasp": "wasp clean && wasp ts-setup",
    "reset:db": "wasp db reset && wasp db migrate-dev",
    "reset:all": "npm run reset:wasp && npm run reset:db",
    "restart:wasp": "npm run reset:wasp && wasp start",
    "restart:db": "npm run reset:db && wasp db start",
    "rebuild:wasp": "rimraf package-lock.json && npm run reset:wasp && wasp build",
    "rebuild:db": "rimraf ./migrations && npm run reset:db",
    "rebuild:all": "npm run rebuild:wasp && npm run rebuild:db",
    "swarm:cli": "npx swarm-cli",
    "swarm:mcp": "npx swarm-mcp",
    "lint": "eslint src *.wasp.ts",
    "lint:fix": "eslint src *.wasp.ts --fix",
    "format": "prettier --write src *.wasp.ts",
    "format:check": "prettier --check src *.wasp.ts",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint:fix && npm run format",
    "swarm": "npm run swarm:cli",
    "cli": "npm run swarm:cli",
    "mcp": "npm run swarm:mcp",
    "dev": "npm run start:wasp",
    "start": "npm run start:wasp",
    "reset": "npm run reset:wasp",
    "rebuild": "npm run rebuild:wasp",
    "restart": "npm run restart:wasp",
    "rs": "npm run reset:wasp",
    "rb": "npm run rebuild:wasp",
    "rt": "npm run restart:wasp",
    "rsw": "npm run reset:wasp",
    "rsd": "npm run reset:db",
    "rta": "npm run reset:all",
    "rbw": "npm run rebuild:wasp",
    "rbd": "npm run rebuild:db",
    "rba": "npm run rebuild:all",
    "rtw": "npm run restart:wasp",
    "rtd": "npm run restart:db",
    "add-ui": "node scripts/add-shadcn.js"
  }
}
